{
    "version": 2,
    "builds": [
      {
        "src": "*.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/public/(.*)",
        "dest": "/public/$1",
        "headers": {
          "Cache-Control": "public, max-age=31536000, must-revalidate"
        }
      },
      {
        "src": "/(.*)",
        "dest": "/$1"
      }
    ]
  }
  