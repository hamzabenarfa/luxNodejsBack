{
    "version": 2,
    "builds": [{ "src": "*.js", "use": "@vercel/node" },
    {
      "src": "public/(.*)",
        "use": [
            "@vercel/static",
            {
            "headers": [
                {
                "source": "/(.*)",
                "headers": [
                    { "key": "Cache-Control", "value": "public, max-age=31536000, must-revalidate" }
                ]
                }
            ]
            }
        ]

    }],
  
    "routes": [{ "src": "/(.*)", "dest": "/" }]
   
  }